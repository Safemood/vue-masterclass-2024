{
  "name": "vue-masterclass-2024",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix --cache",
    "format": "prettier --write src/",
    "superbase:init": "npx supabase init",
    "superbase:login": "npx supabase login",
    "supabase:link": "npx supabase link --project-ref $VITE_SUPABASE_PROJECT_REF",
    "superbase:start": "npx supabase start",
    "migration:make": "supabase migration new",
    "db:migrate": "supabase db push && npm run supabase:types && tsc --noEmit",
    "db:seed": "node --loader ts-node/esm --env-file=.env supabase/seed/runSeeds.js",
    "db:reset": "supabase db reset --linked",
    "supabase:types": "npx supabase gen types typescript --project-id $VITE_SUPABASE_PROJECT_REF --schema public > src/types/types.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.80.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.11",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.37.0",
    "eslint-plugin-vue": "~10.5.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "supabase": "^2.54.11",
    "typescript": "~5.9.0",
    "unplugin-vue-router": "^0.16.1",
    "vite": "^7.1.11",
    "vite-plugin-vue-devtools": "^8.0.3",
    "vue-tsc": "^3.1.1"
  }
}
